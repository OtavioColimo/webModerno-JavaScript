function repeticao(x,repetidor){
array = []
for(let i = 0; i < repetidor; i++)
    array.push(x) 
return console.log(array)
}


repeticao("Codigo",2)



function tabuada(xTabuada){
for(let i=0; i <= 10; i++ ){
   const resultado = xTabuada*i;
    console.log(`${xTabuada} x ${i} = ${resultado}`)}
}


tabuada(7)